int MAGIC_NUMBER;
  
    
    // Input
	std::cout << "Insert a number from 1-10: " ; 
	std::cin >> input_number;
	std::cout << "" << std::endl;
	
	//process
if (input_number == 5) {
	    //output
   std::cout << "Correct! (: ";
}  else {
    std::cout << "Wrong number. The correct number is 5.";
	}
	
	
